// Consulta las sublineas de la tabla de enumeraciones
Sublinea_Enum:
LOAD 
	num([Node Name]) as CodSubLinea, 
	_DisplayName_ as DesSublinea 
FROM
[..\QVD\PLM\Enums_PLM.QVD]
(qvd)
where [Node Name2]='QST_sublinea_enum';

