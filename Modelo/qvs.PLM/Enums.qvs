//Fotos de la Referencia
FotosStyle:
	LOAD 
		Code           as CodReferencia, 
		replace(FilePath,'\Filevault','Z:\') as FotoStyle
	FROM
	[..\QVD\PLM\FotosStyle.QVD]
	(qvd);


